<div id="1RM-template">
  <table>
    <tr>
      <th>ID</th>
      <th>CHEST</th>
      <th>TRICEP</th>
      <th>BICEP</th>
      <th>DEADLIFT</th>
      <th>SQUAT</th>
      <th>PERSONAL NOTES</th>
    </tr>
    {{#each this.records}}
      <tr class="week">
        <form class="individual-form" id="form-form" data-id="{{this.id}}">
          {{#each this}}
            <td><input class="input-this" value="{{this}}" /></td>
          {{/each}}
          <td>
          <button class="delete-record" data-id="{{this.id}}" class="btn-success btn span3">Delete Record</button><button class="update-record" data-id="{{this.id}}" class="btn-success btn span3">Update Record</button></td>
        </form>
      </tr>
    {{/each}}
  </table>
</div>
